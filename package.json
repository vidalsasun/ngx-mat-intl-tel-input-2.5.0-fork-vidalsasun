        "@angular/platform-browser-dynamic": "~8.2.12",
        "@angular/router": "~8.2.11",
        "core-js": "^2.5.4",
        "hammerjs": "^2.0.8",
        "libphonenumber-js": "^1.7.25",
        "rxjs": "~6.5.4",
        "tslib": "^1.9.0",
        "zone.js": "^0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.803.29",
